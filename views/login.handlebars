  <div class="container">
    <h1>Sign up</h1>
    <div class="row">
      <div class="col-sm-12 md-6">
        <form>
          <div class="mb-3">
            <label for="userName" class="form-label">Username</label>
            <input
              type="username"
              class="form-control"
              id="usernameInput"
              aria-describedby="usernameHelp"
            />
          </div>
          <div class="mb-3">
            <label for="InputPassword1" class="form-label"
              >Password</label
            >
            <input
              type="password"
              class="form-control"
              id="InputPassword1"
            />
          </div>
          <button type="submit" class="btn btn-primary">Submit</button>
        </form>
      </div>
      <div class="col-sm-12 md-6">
        <h1>Log in</h1>
        <form>
          <div class="mb-3">
            <label for="inputUsername" class="form-label">Username</label>
            <input
              type="text"
              class="form-control"
              id="inputUsername"
              aria-describedby="usernameHelp"
            />
          </div>
          <div class="mb-3">
            <label for="InputPassword1" class="form-label"
              >Password</label
            >
            <input
              type="password"
              class="form-control"
              id="inputPassword1"
            />
          </div>
          <button type="submit" class="btn btn-primary">Submit</button>
        </form>
      </div>
    </div>
  </div>